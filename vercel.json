{
  "version": 2,
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "buildCommand": "npm run vercel-build",
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
